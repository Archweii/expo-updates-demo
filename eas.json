{
  "cli": {
    "version": ">= 2.1.0",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "production": {
      "channel": "production",
      "autoIncrement": true
    },
    "staging": {
      "channel": "staging",
      "autoIncrement": false
    }
  },
  "submit": {
    "production": {}
  }
}
